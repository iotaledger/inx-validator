{
  "app": {
    "checkForUpdates": true,
    "shutdown": {
      "stopGracePeriod": "5m",
      "log": {
        "enabled": true,
        "filePath": "shutdown.log"
      }
    }
  },
  "logger": {
    "name": "",
    "level": "info",
    "timeFormat": "rfc3339",
    "outputPaths": [
      "stdout"
    ]
  },
  "inx": {
    "address": "localhost:9029",
    "maxConnectionAttempts": 30,
    "targetNetworkName": ""
  },
  "validator": {
    "candidacyRetryInterval": "10s",
    "issueCandidacyPayload": true,
    "ignoreBootstrapped": false,
    "accountAddress": ""
  },
  "profiling": {
    "enabled": false,
    "bindAddress": "localhost:6060"
  }
}
